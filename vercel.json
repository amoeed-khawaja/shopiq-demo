{
    "rewrites": [
        {
            "source": "/save",
            "destination": "/api/save"
        },
        {
            "source": "/register_new",
            "destination": "/api/register_new"
        },
        {
            "source": "/users.json",
            "destination": "/api/users"
        }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, POST, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "Content-Type"
                }
            ]
        }
    ]
}